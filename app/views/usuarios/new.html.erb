<% provide(:titulo, 'Signup')%>
<h1>Sign up</h1>


<%= form_for(@usuario) do |f| %>
	<div class="field">
		<%= f.label :nombre_comunidad %><br />
		<%= f.text_field :nombre_comunidad %>
	</div>
	<div class="field">
		<%= f.label :email %><br />
		<%= f.text_field :email %>
	</div>
	<div class="field">
		<%= f.label :password %><br />
		<%= f.password_field :password %>
	</div>
<% end %>
