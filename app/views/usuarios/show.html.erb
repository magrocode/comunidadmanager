<% provide(:titulo, @usuario.email) %>
<div class="row-fluid">
	<div class="span2">
		<%= render 'menu_usuario' %>
	</div>	
	<div class="span10">
		<div class="content-wrapper">
			<div class="profile-header">
				<%= render 'profile_header' %>
			</div>
			<div class="content">
				<div class="row-fluid">
					<!-- columna de contenidos -->
					<div class="span9">
						<%= render 'notas' %>
					</div>
					<!-- columna de modulos right -->
					<div class="span3 right-module-col">
						<%= render 'show_module_col' %>
					</div>
				</div>
				<div class="clear"></div>
			</div>
		</div>
	</div>
</div>